import random
while True:
    symbol_list = ["1","2","3","4","5","6","7","8","9","0","a","b","c","d""e","f","g","h","i","j","k","l","m","n","o","p","q","r","s","t","u","v","w","x","y","z","[","]","{","}","/","?",".",",",":"]
    password = ""
    lenght = int(input("Длина пароля:"))
    for i in range(0,lenght):
        symbol = random.choice(symbol_list)
        password = password + symbol
    print("Пароль:", password)
    
